<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Home</h1>

    
    Hi <%= user.firstName %> <%= user.lastName %>


    <% if (login) { %>
        <h2>Welcome user</h2>
    <% } else { %>
        <h2> Welcome admin</h2>
    <% } %>   

    
    <% for (const key in students ) { %>
        <h2> <%= key %> - <%= students[key] %> </h2>   
    <% } %>


    <% arr.forEach(item => { %>
        <h2> <%= item.price %> </h2>
    <% }); %>


    <% 
        let html = ''
        for(const item in students) {
            html += '<h3>' + students[item] + '</h3>'
        }
    %>
    <%- html %>
</body>
</html>